{
	"name": "insighthub-frontend",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint . --fix",
		"format": "prettier --write .",
		"test": "vitest",
		"test:watch": "vitest --watch",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest --coverage",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:debug": "playwright test --debug",
		"test:e2e:headed": "playwright test --headed",
		"test:a11y": "playwright test --grep=\"accessible\"",
		"audit:a11y": "playwright test tests/accessibility.spec.ts",
		"test:ai": "playwright test tests/ai-powered-*.spec.ts",
		"test:ai:headed": "playwright test tests/ai-powered-*.spec.ts --headed",
		"test:bug-detection": "playwright test tests/ai-powered-visual.spec.ts --grep='AI Bug Detection'",
		"analyze:ai": "node scripts/analyze-ai-test-results.cjs",
		"test:all": "npm run test && npm run test:e2e && npm run test:ai",
		"ci:test": "npm run test:coverage && npm run test:e2e && npm run test:ai",
		"qa:dashboard": "node scripts/quality-dashboard.js",
		"qa:audit": "npm run lint && npm run check && npm run test:coverage && npm run test:e2e && npm run test:ai && npm run qa:security",
		"qa:security": "npm audit --audit-level=high",
		"qa:performance": "npm run build && npx lighthouse-ci autorun",
		"qa:accessibility": "npm run test:a11y && npx pa11y-ci http://localhost:4173",
		"qa:gates": "npm run qa:audit && npm run qa:dashboard",
		"lint:json": "eslint . --format=json",
		"lint:fix": "eslint . --fix"
	},
	"devDependencies": {
		"@axe-core/playwright": "^4.10.2",
		"@bundle-analyzer/webpack-plugin": "^0.5.1",
		"@playwright/experimental-ct-svelte": "^1.53.1",
		"@playwright/test": "^1.53.1",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.8",
		"@testing-library/user-event": "^14.6.1",
		"@types/jest": "^30.0.0",
		"@types/node": "^24.0.6",
		"@vitest/coverage-v8": "^3.2.4",
		"@vitest/ui": "^3.2.4",
		"autoprefixer": "^10.4.21",
		"axe-core": "^4.10.3",
		"eslint": "^9.29.0",
		"happy-dom": "^18.0.1",
		"jest": "^30.0.3",
		"jest-axe": "^10.0.0",
		"jest-environment-jsdom": "^30.0.2",
		"jsdom": "^26.1.0",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.0.0",
		"vite": "^6.2.6",
		"vite-bundle-analyzer": "^0.23.0",
		"vite-plugin-pwa": "^1.0.0",
		"vitest": "^3.2.4",
		"webpack-bundle-analyzer": "^4.10.2"
	},
	"dependencies": {
		"@supabase/supabase-js": "^2.50.2"
	}
}
